package coursesBasic;

public class IfElseExample {

	public static void main(String[] args) {
		// If Example
		int age = 21;
		if (age > 18) {
			System.out.println("Age Must Be Greater Than 18");
		}
		// if-else example
		int a = 15;
		if (a * 2 == 30) {
			System.out.println("The Answer is Correct!");
		} else {
			System.out.println("Try Again...");
		}

	}

}
