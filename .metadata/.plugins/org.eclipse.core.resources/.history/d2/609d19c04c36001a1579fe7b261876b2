
public class StringEndsWithExample {

	public static void main(String[] args) {

		String str = "News Paper";
		System.out.println("+ " + str.endsWith("r") + " +");
		System.out.println("- " + str.endsWith("paper") + " -");
		//
		//
		String str1 = "I work better at nights!";
		System.out.println("xx " + str1.endsWith("onion") + " xx");
		if (str1.endsWith("nights!")) {
			System.out.println(" ++" + "String ends with nights!");
		} else
			System.out.println("xx " + "Not Applicable" + " xx");

	}

}
