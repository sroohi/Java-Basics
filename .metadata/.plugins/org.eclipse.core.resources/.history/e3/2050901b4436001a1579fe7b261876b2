package stringPackage;

public class StringCharAtExample {

	public static void main(String[] args) {

		String abc = "Java Programming";
		char a = abc.charAt(8);
		System.out.println(a); // "g" is the 8th index

		String str = "Shawn Roohi";
		char b = str.charAt(3);
		System.out.println(b);// "w" is the 3d index

	}

}
